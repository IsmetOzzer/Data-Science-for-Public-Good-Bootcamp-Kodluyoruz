   "source": [
    "l = [[\"a\",\"b\"],\"c\",[\"a\",\"d\"],[\"a\"],\"c\"]\n",
    "\n",
    "d = {}\n",
    "\n",
    "def Counter(l):\n",
    "    \n",
    "    l_unnest = [item for sublist in l for item in sublist]\n",
    "    \n",
    "    for i in l_unnest:\n",
    "        if i not in d:\n",
    "            d[i] = 1\n",
    "        else:\n",
    "            d[i] += 1\n",
    "    return d\n",
    "        \n",
    "Counter(l)  \n"
   ]
  }
